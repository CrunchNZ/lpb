# Liquidity Sentinel Environment Configuration
# Copy this file to .env and fill in your actual values

# =============================================================================
# API Configuration
# =============================================================================

# DexScreener API Configuration
VITE_DEXSCREENER_API_URL=https://api.dexscreener.com
VITE_DEXSCREENER_API_KEY=your_dexscreener_api_key_here

# Jupiter API Configuration
VITE_JUPITER_API_URL=https://quote-api.jup.ag
VITE_JUPITER_API_KEY=your_jupiter_api_key_here

# Meteora API Configuration
VITE_METEORA_API_URL=https://api.meteora.ag
VITE_METEORA_API_KEY=your_meteora_api_key_here

# Raydium API Configuration
VITE_RAYDIUM_API_URL=https://api.raydium.io
VITE_RAYDIUM_API_KEY=your_raydium_api_key_here

# Orca API Configuration
VITE_ORCA_API_URL=https://api.orca.so
VITE_ORCA_API_KEY=your_orca_api_key_here

# =============================================================================
# Database Configuration
# =============================================================================

# SQLite Database Configuration
VITE_DATABASE_URL=./data/liquidity_sentinel.db
VITE_DATABASE_MIGRATIONS_PATH=./src/backend/database/migrations

# =============================================================================
# Security Configuration
# =============================================================================

# Sentry Error Tracking
VITE_SENTRY_DSN=your_sentry_dsn_here
VITE_ENABLE_SENTRY=true

# Analytics Configuration
VITE_ENABLE_ANALYTICS=true
VITE_ANALYTICS_ID=your_analytics_id_here

# Security Settings
VITE_ENABLE_HTTPS_ONLY=true
VITE_ENABLE_CSP=true
VITE_ENABLE_XSS_PROTECTION=true

# =============================================================================
# Feature Flags
# =============================================================================

# Core Features
VITE_ENABLE_SWAP=true
VITE_ENABLE_LIQUIDITY=true
VITE_ENABLE_WATCHLISTS=true
VITE_ENABLE_POSITIONS=true
VITE_ENABLE_ANALYTICS=true

# Advanced Features
VITE_ENABLE_SENTIMENT_ANALYSIS=true
VITE_ENABLE_MEV_PROTECTION=true
VITE_ENABLE_MULTI_CHAIN=true
VITE_ENABLE_SOCIAL_FEATURES=true

# Development Features
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_HOT_RELOAD=true
VITE_ENABLE_DEV_TOOLS=true

# =============================================================================
# Network Configuration
# =============================================================================

# Solana Network Configuration
VITE_SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
VITE_SOLANA_DEVNET_URL=https://api.devnet.solana.com
VITE_SOLANA_TESTNET_URL=https://api.testnet.solana.com

# Wormhole Configuration (for multi-chain)
VITE_WORMHOLE_RPC_URL=https://wormhole-v2-mainnet.certus.one
VITE_WORMHOLE_CHAIN_ID=1

# =============================================================================
# Trading Configuration
# =============================================================================

# Strategy Configuration
VITE_DEFAULT_SLIPPAGE=0.5
VITE_MAX_SLIPPAGE=5.0
VITE_MIN_MCAP_THRESHOLD=150000
VITE_MAX_POSITION_SIZE=10000

# Risk Management
VITE_ENABLE_RUG_CHECK=true
VITE_ENABLE_IMPERMANENT_LOSS_PROTECTION=true
VITE_MAX_IMPERMANENT_LOSS=10.0

# =============================================================================
# Social Media APIs
# =============================================================================

# Twitter API Configuration
VITE_TWITTER_API_KEY=your_twitter_api_key_here
VITE_TWITTER_API_SECRET=your_twitter_api_secret_here
VITE_TWITTER_BEARER_TOKEN=your_twitter_bearer_token_here

# Telegram API Configuration
VITE_TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
VITE_TELEGRAM_CHANNEL_ID=your_telegram_channel_id_here

# Reddit API Configuration
VITE_REDDIT_CLIENT_ID=your_reddit_client_id_here
VITE_REDDIT_CLIENT_SECRET=your_reddit_client_secret_here

# =============================================================================
# Performance Configuration
# =============================================================================

# Caching Configuration
VITE_CACHE_TTL=300000
VITE_MAX_CACHE_SIZE=100
VITE_ENABLE_CACHE=true

# Rate Limiting
VITE_API_RATE_LIMIT=100
VITE_API_RATE_LIMIT_WINDOW=60000

# =============================================================================
# UI Configuration
# =============================================================================

# Theme Configuration
VITE_DEFAULT_THEME=system
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_ANIMATIONS=true

# Accessibility
VITE_ENABLE_SCREEN_READER_SUPPORT=true
VITE_ENABLE_KEYBOARD_NAVIGATION=true
VITE_ENABLE_HIGH_CONTRAST=true

# =============================================================================
# Development Configuration
# =============================================================================

# Development Server
VITE_DEV_SERVER_PORT=3000
VITE_DEV_SERVER_HOST=localhost

# Hot Reload
VITE_ENABLE_HOT_RELOAD=true
VITE_HOT_RELOAD_PORT=24678

# =============================================================================
# Testing Configuration
# =============================================================================

# Test Environment
VITE_TEST_MODE=false
VITE_MOCK_API_RESPONSES=false
VITE_ENABLE_TEST_LOGGING=false

# =============================================================================
# Deployment Configuration
# =============================================================================

# Build Configuration
VITE_BUILD_SOURCEMAP=true
VITE_BUILD_MINIFY=true
VITE_BUILD_TARGET=esnext

# CDN Configuration
VITE_CDN_URL=https://cdn.liquiditysentinel.com
VITE_ASSETS_PATH=/assets

# =============================================================================
# Monitoring Configuration
# =============================================================================

# Health Check
VITE_HEALTH_CHECK_INTERVAL=30000
VITE_ENABLE_HEALTH_CHECKS=true

# Performance Monitoring
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_PERFORMANCE_THRESHOLD=3000

# Error Reporting
VITE_ENABLE_ERROR_REPORTING=true
VITE_ERROR_REPORTING_URL=https://errors.liquiditysentinel.com 